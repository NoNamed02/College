/*
단어를 저장하는 연결 리스트 제작
*/

#include<stdio.h>
#include<stdlib.h>
#include<string.h>

typedef struct Node{
    char name[100];
    struct Node *link;
}Node;

 Node* insert_f(Node* head, char n[]){
     Node* p = (Node*)malloc(sizeof(Node));
     strcpy(p->name,n);
     p->link = head;
     head = p;
     return head;
 }

void print(Node* head){
    Node* p;
    for(p=head;p!=NULL;p=p->link)
        printf("%s -> ", p->name);
    printf("NULL\n");
}

int main(void){
    Node *A = (Node*)malloc(sizeof(Node));
    strcpy(A->name,"APPLE");
    A->link = NULL;
    print(A);
    
    A = insert_f(A,"KIWI");
    print(A);
    
    A = insert_f(A,"BANANA");
    print(A);
    
    return 0;
}
